    <blockTable colWidths="10cm,9cm" style="Table_String">
      	<tr>
      		<td>
      			<illustration width="2 cm" height="1 cm">
				     <image x="5" y="5" width="2 cm" height="1 cm">[[ data.company_id.logo ]]</image>
				</illustration>
      		</td>
      		<td>
      			<para style="terp_header">Delivery Order</para>
      		</td>      		
      	</tr>      	
    </blockTable>    
	
    <!-- Membuat Header Report -->
	
	<blockTable colWidths="520.0" style="Table_String">
		<tr>
			<td><para style="terp_default">Kepada Yth. [[ data.partner_id.name ]] di [[ data.partner_id.street ]] [[ data.partner_id.city_id.name ]] [[ data.partner_id.state_id.name ]] [[ data.partner_id.zip ]]</para></td>
		</tr>
	</blockTable>
	<blockTable colWidths="520.0" style="Table_String">
		<tr>
			<td><para style="terp_default">Ekspedisi : [[ data.expedition_id.name ]], Alamat : [[ data.expedition_id.street ]] [[ data.expedition_id.city_id.name ]] [[ data.expedition_id.state_id.name ]] [[ data.expedition_id.zip ]]</para></td>
		</tr>
	</blockTable>
