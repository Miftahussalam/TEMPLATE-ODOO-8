<?xml version="1.0" encoding="utf-8"?>
<document filename="Delivery Order.pdf">
	
	<!-- DEFINISI LAYOUT (Ukuran Kertas) -->
	
  	<template pageSize="(21cm, 10cm)" 
	          leftMargin="2.5cm"
              rightMargin="2.5cm"
              topMargin="1cm"
              bottomMargin="1.5cm"
	title="Faktur Penjualan" author="PT. Witaco">
		<pageTemplate id="main">
			<frame id="first" x1="1.5cm" y1="1cm" width="20cm" height="9cm"/>
			<header>
				<pageGraphics>
					<setFont name="Helvetica" size="5"/>
				</pageGraphics>
			</header>			
		</pageTemplate>
	</template>
   
