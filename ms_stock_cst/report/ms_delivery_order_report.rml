    <!-- Membuat Looping Data Tabel Peserta (Element Section) -->
     
    <section>
       
      	<para style="terp_default">[[ repeatIn(data.move_lines, 'line') ]]</para>
       
      		<blockTable colWidths="40.0,300.0,50.0,70.0,50.0" repeatRows="0" style="List_Order_Reference_Tbl">
		        <tr>
		        	<td><para style="terp_default">[[ no_urut() ]]</para></td>
					<td><para style="terp_default">[[ line.product_id.name ]]</para></td>
					<td><para style="terp_default">[[ formatLang(line.product_qty, digits=0) ]]</para></td>
					<td><para style="terp_default">[[ line.product_uom.name ]]</para></td>
